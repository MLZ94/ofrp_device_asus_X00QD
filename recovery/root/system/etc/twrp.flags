# mount_point   fstype    device                                             flags
/boot            emmc	/dev/block/bootdevice/by-name/boot
/recovery	 emmc   /dev/block/bootdevice/by-name/recovery	 flags=backup=1       
/dtbo            emmc   /dev/block/bootdevice/by-name/dtbo       flags=display="DTBO";backup=1
/system          ext4	/dev/block/bootdevice/by-name/system	 flags=display="System";wipeingui
/vendor          ext4   /dev/block/bootdevice/by-name/vendor     flags=display="Vendor";wipeingui

/system_image	 emmc   /dev/block/bootdevice/by-name/system     flags=slotselect;flashimg=1
/vendor_image    emmc   /dev/block/bootdevice/by-name/vendor     flags=slotselect;backup=1;flashimg=1
/persist_image	 emmc 	/dev/block/bootdevice/by-name/persist 	 flags=display="Persist";backup=1;flashimg=1

/data            ext4   /dev/block/bootdevice/by-name/userdata   flags=fileencryption=ice;encryptable=footer
/cache           ext4   /dev/block/bootdevice/by-name/cache
/firmware        vfat   /dev/block/bootdevice/by-name/modem      flags=slotselect;display="Firmware";mounttodecrypt;fsflags=ro
/misc            emmc   /dev/block/bootdevice/by-name/misc
/modem           emmc   /dev/block/bootdevice/by-name/modem      flags=slotselect;backup=1;display="Modem"
/bluetooth       emmc   /dev/block/bootdevice/by-name/bluetooth  flags=slotselect;backup=1;subpartitionof=/modem
/dsp             emmc   /dev/block/bootdevice/by-name/dsp        flags=slotselect;backup=1;subpartitionof=/modem
/efs1            emmc   /dev/block/bootdevice/by-name/modemst1   flags=backup=1;display=EFS
/efs2            emmc   /dev/block/bootdevice/by-name/modemst2   flags=backup=1;subpartitionof=/efs1
/efsc            emmc   /dev/block/bootdevice/by-name/fsc        flags=backup=1;subpartitionof=/efs1
/efsg            emmc   /dev/block/bootdevice/by-name/fsg        flags=backup=1;subpartitionof=/efs1

/asdf           ext4      /dev/block/bootdevice/by-name/asdf                 flags=display="ASDF"
/factory        ext4      /dev/block/bootdevice/by-name/factory              flags=display="Factory";backup=1
/firmware       vfat      /dev/block/bootdevice/by-name/modem                flags=display="Modem firmware"
/persist        ext4      /dev/block/bootdevice/by-name/persist              flags=display="Persist";backup=1
/asusfw         ext4      /dev/block/bootdevice/by-name/asusfw               flags=display="ASUS firmware"
/dsp            ext4      /dev/block/bootdevice/by-name/dsp                  flags=display="DSP"
/xrom           ext4      /dev/block/bootdevice/by-name/xrom                 flags=display="ASUS XROM";backup=1

# Removable storage
/usb_otg        auto   /dev/block/sda1             /dev/block/sda      flags=display="USB-OTG";wipeingui;storage;removable
/external_sd    auto   /dev/block/mmcblk1p1        /dev/block/mmcblk1  flags=display="MicroSD";wipeingui;removable
